{
  "name": "course-plans",
  "version": "0.0.1",
  "description": "Fetch and parse course plans from various institutes",
  "main": "index.js",
  "scripts": {
    "dev": "DEBUG=course-plans:* node src/index.js",
    "start": "DEBUG=course-plans:* node src/index.js",
    "lint": "xo",
    "test": "ava",
    "coverage": "nyc ava -t",
    "check-duplicate-code": "jsinspect -t 20 src",
    "build": "./build.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "jsinspect": "^0.12.7",
    "nyc": "^13.3.0",
    "xo": "^0.24.0"
  },
  "xo": {
    "space": 2,
    "esnext": true,
    "rules": {
      "curly": [
        "error",
        "multi-or-nest",
        "consistent"
      ],
      "no-use-before-define": [
        "error",
        {
          "classes": false
        }
      ],
      "unicorn/catch-error-name": [
        "error",
        {
          "name": "error"
        }
      ],
      "dot-notation": [
        "off"
      ],
      "quote-props": [
        "off"
      ],
      "object-curly-spacing": [
        "off"
      ],
      "no-new": [
        "off"
      ],
      "padding-line-between-statements": [
        "off"
      ]
    }
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "concurrency": 5,
    "failFast": false,
    "verbose": true,
    "compileEnhancements": false
  }
}
